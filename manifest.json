{
    "manifest_version": 3,
    "name": "GPT Test Extension",
    "description": "This is a very cool extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup_htmls/popup.html",
      "default_icon": "icons/extension_icon.png"
    },
    "content_scripts": [
        {
          "js": [
            "scripts/fa_initiator.js",
            "scripts/fa_utils.js",
            "scripts/fa_listeners.js",
            "scripts/fa_ai.js",
            "scripts/fa_rewrite_handle.js",
            "scripts/fa_actions.js"
          ],
          "css": [
            "css/ui.css"
          ],
          "matches": [
            "https://*/*"
          ]
        }
    ]
}
